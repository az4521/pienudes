        <div class="col-lg-12 col-md-12">

            <div id="queue-header" class="card">
                <div id="plmeta">
                    <div class="input-group">
                        <input id="mediaurl" class="form-control" type="text" placeholder="Play something... (Links from YouTube, SoundCloud)" />
                        <span class="input-group-btn">
                            <button id="queue_end" class="btn btn-default" style="padding-right: 0;">
                                <span class="glyphicon glyphicon-plus"></span> Add
                            </button>
                        </span>
                    </div>
                    <div class="plbuttons">
                        <button id="showsearch" class="btn btn-sm btn-default" title="Search for a video" data-toggle="collapse" data-target="#searchcontrol">
                            <span class="glyphicon glyphicon-search"></span> Search
                        </button>
                        <button id="clearplaylist" class="btn btn-sm btn-default" title="Clear the playlist">
                            <span class="glyphicon glyphicon-trash"></span> Clear
                        </button>
                        <button id="shuffleplaylist" class="btn btn-sm btn-default" title="Shuffle the playlist">
                            <span class="glyphicon glyphicon-sort"></span> Shuffle
                        </button>
                        <button id="qlockbtn" class="btn btn-sm btn-default" title="Playlist locked">
                            <span class="glyphicon glyphicon-lock"></span> Lock
                        </button>
                        <button id="showcustomembed" class="btn btn-sm btn-default" title="Embed a custom frame" data-toggle="collapse" data-target="#playlistmanager">
                            <span class="glyphicon glyphicon-th-large"></span> IFrame
                        </button>
                    </div>
                    <!--
                    <span id="plcount">0 items</span>
                    <span id="pllength">00:00:00</span>
                    -->
                </div>
            </div>
            <div id="video-playlist" class="card">
                <table>
                    <thead>
                        <tr>
                            <th class="video-playlist-actions"></th>
                            <th class="video-playlist-title">Title</th>
                            <th class="video-playlist-time">Time</th>
                            <th class="video-playlist-queueby">QueuedBy</th>
                        </tr>
                    </thead>
                    <tbody>
                    
                    </tbody>
                </table>
            </div>
        </div>
