<div id="emotelist" class="modal fade" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-dialog-nonfluid">
        <div class="modal-content">
            <div class="modal-header">
                <button class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4>Emotes</h4>
            </div>
            <div class="modal-body">
                <div class="pull-left">
                    <input class="emotelist-search form-control" type="text" placeholder="Search" />
