<!DOCTYPE html>
<html lang="en">
    <head>
        {% include "head.html.twig" %}
    </head>
    <body>
        <div id="wrap" class="profile-wrap">
            {% include "nav/default.html.twig" %}
            <section id="mainpage" class="profile-mainpage">
                <div class="container-fluid padding-remove">
                    {% block content %}{% endblock %}
                </div>
            </section>
        </div>
        {% include "foot.html.twig" %}
        <script src="/js/ga.js"></script>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.2/jquery.min.js"></script>
        <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.4/jquery-ui.min.js"></script>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/js/bootstrap.min.js"></script>
        <script src="/js/spectrum.js?v={{ cacheBuster }}"></script>
        <script src="/js/profile.js?v={{ cacheBuster }}"></script>
        <script>
            $("#channelname").keydown(function (ev) {
                if (ev.keyCode === 13) {
                    location.href = "/r/" + $("#channelname").val();
                }
            });
        </script>
        {% if pageScripts %}
            {% for script in pageScripts %}
                <script src="{{ script }}?v={{ cacheBuster }}"></script>
            {% endfor %}
        {% endif %}
    </body>
</html>