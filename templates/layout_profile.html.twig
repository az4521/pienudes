<!DOCTYPE html>
<html lang="en">
<head>
    {% include "head.html.twig" %}
</head>
<body>
<div id="wrap" class="profile-wrap">
    {% include "nav/default.html.twig" %}
    <section id="mainpage" class="profile-mainpage">
        <div class="container-fluid padding-remove">
            {% block content %}{% endblock %}
        </div>
    </section>
</div>
{% include "foot.html.twig" %}
<script src="/js/ga.js"></script>
<script src="/js/jquery-1.11.0.min.js"></script>
<!-- Must be included before jQuery-UI since jQuery-UI overrides jQuery.fn.button-->
<script src="/js/jquery-ui.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/js/bootstrap.min.js"></script>
<script src="/js/spectrum.js?v={{ cacheBuster }}"></script>
<script src="/js/profile.js?v={{ cacheBuster }}"></script>
<script>
    $("#channelname").keydown(function (ev) {
        if (ev.keyCode === 13) {
            location.href = "/r/" + $("#channelname").val();
        }
    });
</script>
{% if pageScripts %}
    {% for script in pageScripts %}
        <script src="{{ script }}?v={{ cacheBuster }}"></script>
    {% endfor %}
{% endif %}
<!-- Twitter universal website tag code -->
<script src="//platform.twitter.com/oct.js" type="text/javascript"></script>
<script type="text/javascript">twttr.conversion.trackPid('nuwod', { tw_sale_amount: 0, tw_order_quantity: 0 });</script>
<noscript>
    <img height="1" width="1" style="display:none;" alt="" src="https://analytics.twitter.com/i/adsct?txn_id=nuwod&p_id=Twitter&tw_sale_amount=0&tw_order_quantity=0" />
    <img height="1" width="1" style="display:none;" alt="" src="//t.co/i/adsct?txn_id=nuwod&p_id=Twitter&tw_sale_amount=0&tw_order_quantity=0" />
</noscript>
<!-- End Twitter universal website tag code -->
</body>
</html>